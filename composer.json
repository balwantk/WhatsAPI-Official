    "license": "MIT",
    "keywords": [
        "WhatsApp",
        "TextMessage",
        "sms"
    ],
    "authors": [
        {
            "role": "Contributers",
            "name": "WhatsApi Contributing Team",
            "homepage": "https://github.com/mgp25/WhatsAPI-Official/graphs/contributors"
        }
    ],
    "support": {
        "issues": "https://github.com/mgp25/WhatsAPI-Official/issues",
